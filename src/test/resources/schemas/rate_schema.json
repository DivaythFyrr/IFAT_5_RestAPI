{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RateResponse",
  "type": "object",
  "required": [
    "amount",
    "grow",
    "scale"
  ],
  "properties": {
    "amount": {
      "type": "string",
      "pattern": "^\\d+,\\d{4}$"
    },
    "grow": {
      "type": "integer"
    },
    "scale": {
      "type": "integer"
    }
  }
}
